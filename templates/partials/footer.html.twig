    <div class="container pt-0 pb-0">
        <div class="row mb-4 justify-content-center">
            <div class="col-auto">
            <a href="{{ path('app_home') }}">
                <img class="logo-img" src="{{ asset('build/images/logo.png') }}" alt="16/9" />
            </a>
            </div>
        </div>
        <div class="row mb-4">
            <div class="col">
            <ul class="nav footer-nav justify-content-center">
                <li class="nav-item"><a href="mailto:fanny@seize9eme.fr" class="nav-link">Contact</a>
                </li>
                <li class="nav-item"><a href="{{ path('app_legal_notice') }}" class="nav-link">Mentions Légales</a>
                </li>
                <li class="nav-item"><a href="{{ path('app_privacy_policy') }}" class="nav-link">Politique de confidentialité</a>
                </li>
            </ul>
            </div>
        </div>
    </div>
</section>
